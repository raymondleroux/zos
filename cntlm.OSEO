//IOACLVLC JOB (ACCT),CNTLM,MSGCLASS=S,
//      NOTIFY=&SYSUID,CLASS=A,MSGLEVEL=(1,1),TIME=(0,10)
//*SET ENV=P1
//*SET ENV=R1
// SET ENV=U1
//MYLIB JCLLIB ORDER=(S28P.&ENV..IOA920.PROCLIB,
// SYS1.SACBCNTL)
// INCLUDE MEMBER=IOAENV&ENV.
SYS4.IOA920.BASE.GENERALV
//STEP1   EXEC IOACLVLC,IN1=S28P.U1.IOA920.PARM,
// IN2=S28P.U1.IOA918.PARM,
// OUTDSN=&SYSUID..IOA920.PARMCOMP
//
//********************************************************************
//*
//* THE PROCEDURE COMPARES BETWEEN TWO REPORTS GENERATED BY THE
//* JOB 'IOACLVLG' TO FIND THE DIFFERENCES BETWEEN TWO INSTALLATIONS
//*
//* THE PARAMETERS:
//*       'OUTDSN' - THE VARIABLE DEFINES THE DATA SET NAME
//*                  OF THE RESULTING REPORT
//*       'IN1'    - THE VARIABLE DEFINES THE DSNAME OF THE REPORT THAT
//*                  GENERATED BY THE JOB 'IOACLVLG' ON THE FIRST
//*                  INSTALLATION TO BE COMPARED
//*       'IN2'    - THE VARIABLE DEFINES THE DSNAME OF THE REPORT THAT
//*                  GENERATED BY THE JOB 'IOACLVLG' ON THE SECOND
//*                  INSTALLATION TO BE COMPARED
//*
//********************************************************************
//IOACLVLC PROC IN1=,IN2=,OUTDSN=
//*-------- DETETE THE OUTPUT DATA SET IF EXISTS --------------
//CLEANUP  EXEC PGM=IEFBR14
//DELDS    DD DSN=&OUTDSN,DISP=(MOD,DELETE,DELETE),
//            SPACE=(TRK,(2000,20))
//*--------  MERGE THE LEVEL INFO OF TWO INPUT DATA SETS -------------
//MERGE    EXEC PGM=SORT
//SORTIN01 DD DISP=SHR,DSN=&IN1
//SORTIN02 DD DISP=SHR,DSN=&IN2
//SORTOUT  DD DISP=(NEW,PASS,DELETE),SPACE=(TRK,(2000,200),RLSE),
//            RECFM=FB,LRECL=49,DSN=&&MERGEOUT
//SYSOUT   DD SYSOUT=*
//DFSPARM  DD DISP=SHR,DSN=&ILPREFA..IOAENV(IOACLVLS)
//SYSABEND DD SYSOUT=*
//*-------- EXTRACT UNIQUE ENTRIES -----------------------------------
//GETUNIQ  EXEC PGM=ICETOOL,REGION=1024K
//TOOLMSG  DD SYSOUT=*
//DFSMSG   DD SYSOUT=*
//TOOLIN   DD DISP=SHR,DSN=&ILPREFA..IOAENV(IOACLVLI)
//ICEIN    DD DISP=(SHR,PASS,DELETE),DSN=*.MERGE.SORTOUT
//ICEOUT   DD DISP=(NEW,CATLG,DELETE),SPACE=(TRK,(2000,200),RLSE),
//            RECFM=FB,LRECL=49,DSN=&OUTDSN
//         PEND


./ ADD NAME=IOACLVLG
//IOACLVLG JOB (ACCT),CNTLM,MSGCLASS=S,
//      NOTIFY=&SYSUID,CLASS=A,MSGLEVEL=(1,1),TIME=(0,10)
//*SET ENV=P1
//*SET ENV=R1
// SET ENV=U1
//MYLIB JCLLIB ORDER=(S28P.&ENV..IOA920.PROCLIB,
// SYS1.SACBCNTL)
//STEP1    EXEC IOACLVLC,SETID=920,
// OUTDSN=&SYSUID..IOA920.IOACLVLG
// INCLUDE MEMBER=IOAENV&ENV.
// INCLUDE MEMBER=IOASET&ENV.
//
//********************************************************************
//*
//* THE PROCEDURE COLLECTS THE LEVEL INFORMATION OF ALL LOAD MODULES
//* IN THE STEPLIB DEFINED BY THE INSTALLATION (IN THE MEMBER 'IOASET')
//*
//* THE PARAMETERS:
//*       'SETID'  - DEFINES UP TO 8 CHARACTER IDENTIFICATOR OF THE
//*                  INSTALLATION - USED TO DISTINGUISH BETWEEN THE
//*                  REPORTS GENERATED ON DIFFERENT SYSTEMS
//*                  (FOR EXAMPLE 'TEST' OR 'PROD')
//*
//*       'OUTDSN' - DEFINES THE DATA SET NAME OF THE RESULTING REPORT
//*
//********************************************************************
//*===================================================================
//IOACLVLG PROC SETID=,OUTDSN=
//*-------- DETETE THE OUTPUT DATA SET IF EXISTS ---------------------
//CLEANUP  EXEC PGM=IEFBR14
//DELDS    DD DSN=&OUTDSN,DISP=(MOD,DELETE,DELETE),SPACE=(TRK,(1,1))
//SYSABEND DD SYSOUT=*
//*-------- GET LEVEL INFORMATION ------------------------------------
//GETLVL   EXEC PGM=IOAGLVL,REGION=5M,PARM='&SETID'
//         INCLUDE MEMBER=&IOAENV
//DAPRMDF  DD DISP=(NEW,PASS,DELETE),DSN=&&DFSPRM,LRECL=80,RECFM=FB,
//            SPACE=(TRK,(1,0),RLSE),BLKSIZE=800,DSORG=PS
//DATRACE  DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//DAPRIDL  DD DISP=(NEW,PASS,DELETE),SPACE=(TRK,(1000,100),RLSE),
//            DSN=&&LVLOUT
//*-------- SORT AND INSERT INPUT DATA SET NAME AND MCTQNAME ---------
//INSDSNQN EXEC PGM=SORT
//SORTIN   DD DISP=(SHR,DELETE,DELETE),DSN=*.GETLVL.DAPRIDL
//SORTOUT  DD DISP=(NEW,PASS,DELETE),SPACE=(TRK,(1000,100),RLSE),
//            LRECL=49,RECFM=FB,DSN=&&SORTOUT
//DFSPARM  DD DISP=(SHR,DELETE,DELETE),DSN=*.GETLVL.DAPRMDF
//SYSOUT   DD SYSOUT=*
//SYSABEND DD SYSOUT=*
//*-------- REMOVE DUPLICATE ENTRIES ---------------------------------
//RMVDUP   EXEC PGM=ICETOOL,REGION=1024K
//TOOLMSG  DD SYSOUT=*
//DFSMSG   DD SYSOUT=*
//TOOLIN   DD DISP=SHR,DSN=&ILPREFA..IOAENV(IOACLVLT)
//ICEIN    DD DISP=(SHR,DELETE,DELETE),DSN=*.INSDSNQN.SORTOUT
//ICEOUT   DD DISP=(NEW,CATLG,DELETE),SPACE=(TRK,(2000,200),RLSE),
//            RECFM=FB,LRECL=49,DSN=&OUTDSN
//         PEND
//*===================================================================


